    "version": "0.2.0",
    "configurations": [


        {
            "name": "Python Debugger: zabbix_iber.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "credentials.ini",
                "--day",
                "10-11-2024"
            ]
        },
        {
            "name": "Python Debugger: month_iber.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "credentials.ini",
                "--start",
                "01-01-2024",
                "--end",
                "01-02-2024"
            ]
        },
        {
            "name": "Python Debugger: zabbix_month_csv.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "credentials.ini",
                "--file",
                "consumo_luz_2024_01.csv"
            ]
        },
        {
            "name": "Python Debugger: test.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "4",
                "-v",
                "1"
            ]
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "${command:pickArgs}"
            ]
        }
    ]
}